        "@fastify/type-provider-typebox": "^5.1.0",
        "@sinclair/typebox": "^0.34.33",
        "@types/pg": "^8.15.2",
        "bcryptjs": "^3.0.2",
        "dotenv": "^16.5.0",
        "fastify": "^5.0.0",
        "fastify-cli": "^7.4.0",
        "fastify-plugin": "^5.0.0",
        "pg": "^8.16.0",
        "pg-pool": "^3.10.0",
        "pino": "^9.7.0",
